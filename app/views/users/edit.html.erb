<div class="contents row">
  <div class="container">
    <%= form_for current_user do |f| %>
      <h3>登録情報</h3>


      <div class="field">
      <%= f.label "画像" %><br>
      <%= f.file_field :image %>
      </div>

      <div class="form-group">
        <%= f.submit "更新", class: "btn btn-primary withripple" %>
      </div>

    <% end %>
  </div>
</div>